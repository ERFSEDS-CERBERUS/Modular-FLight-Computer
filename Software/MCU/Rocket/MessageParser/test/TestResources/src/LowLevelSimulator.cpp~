#include "LowLevelSimulator.hpp"


LowLevelSimulator::LowLevelSimulator();
void arm_callback(void * context, bool arm);
void fire_callback(void * context, int channel);
void flash_write_callback(void* context, const char * msg, const size_t size);
void transmit_callback(void* context, const char * msg, const size_t size);
			 
void setArmed(bool armed);
void setFired(int index, bool status);
bool getArmed();
bool getFired(int index);
